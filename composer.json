{
    "name": "juizmill/wp-base",
    "description": "MÃ³dulo base para criar APP com Zend Framework 2",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "module",
        "zf2",
        "WebPatterns",
        "base"
    ],
    "homepage": "http://blog.webpatterns.com.br",
    "authors": [
        {
            "name": "Jesus Vieira",
            "email": "jesusvieiradelima@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zend-mvc": "2.*",
        "zendframework/zend-serializer": "2.*",
        "zendframework/zend-version": "2.*",
        "zendframework/zend-form": "2.*",
        "zendframework/zend-mail": "2.*",
        "zendframework/zend-mime": "2.*",
        "zendframework/zend-test": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "doctrine/doctrine-orm-module": "0.8.0",
        "spiffy/spiffy-test": "0.1.4",
        "phpunit/phpunit": "3.7.x-dev"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "zendframework/zend-developer-tools": "zend-developer-tools if you want to profile operations executed by the ORM during development"
    },
    "autoload": {
        "psr-0": {
            "WPBase\\": "src/",
            "SpiffyRoutesTest": "test/"
        }
    }

}
